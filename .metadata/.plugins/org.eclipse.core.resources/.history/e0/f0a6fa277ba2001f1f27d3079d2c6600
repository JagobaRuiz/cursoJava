package com.ipartek.bases;

import java.util.Arrays;

public class ArraysEjemplo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int[] arr = new int[3];
		arr[0] = 10;
		arr[1] = 20;
		arr[2] = 30;
		
		arr[3] = 40;
		
		System.out.println(Arrays.toString(arr));
	}

}
